{
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-plusplus": 0,
      "no-path-concat": 0,
      "prefer-template": 0,
      "arrow-body-style": 0,
      "no-restricted-syntax": 0,
      "prefer-const": 0,
      "consistent-return": 0,
      "no-unused-expressions": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-alert": 0,
      "no-console": 0,
      "function-paren-newline": 0,
      "array-callback-return": 0,
      "import/prefer-default-export": 0,
      "radix": 0
    },
    "env": {
      "browser": true,
      "jquery": true
    }
}
